hostname MS-MR
!
username admin privilege 15 password versa123
!
interface Loopback0
 ip address 3.3.3.3 255.255.255.255
 ip ospf 1 area 0
!
interface GigabitEthernet2
 ip address 11.11.11.3 255.255.255.0
 ip ospf 1 area 0
 negotiation auto
 no mop enabled
 no mop sysid
!
interface GigabitEthernet3
 ip address 11.11.22.3 255.255.255.0
 ip ospf 1 area 0
 negotiation auto
 no mop enabled
 no mop sysid
!
router lisp
 locator-set RLOC_SET
  3.3.3.3
  exit-locator-set
 !
 service ipv4
  map-server
  map-resolver
  exit-service-ipv4
 !
 site default
  authentication-key cisco
  eid-record 0.0.0.0/0 accept-more-specifics
  exit-site
 !
 exit-router-lisp
!
router ospf 1
 router-id 3.3.3.3
!
line vty 0 4
 login local
 transport input ssh
!
